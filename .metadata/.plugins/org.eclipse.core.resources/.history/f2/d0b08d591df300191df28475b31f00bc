package xyz.ysy520.arrays;

public class Test01 {
	public static void main(String[] args) {
		int[] arr1 = null; //声明数组
		arr1 = new int[10]; //给数组分配空间
		String[] arr2 = new String[10];
		
		arr1[0] = 115;
		arr1[1] = 120;
		
		for (int i = 0; i < arr1.length; i++) {
			System.out.println(arr1[i]);
		}
	}
}