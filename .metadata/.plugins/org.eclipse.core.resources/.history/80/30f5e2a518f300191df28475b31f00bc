 */
public class TestPolym {
	public static void main(String[] args) {
		Dog d = new Dog();
		Cat c = new Cat();
		animalShout(d);
		animalShout(c);
	}
	
	static void animalShout(Animal a) {
		a.shout();
	}
}

class Animal {
	public void shout () {
		System.out.println();
	}
}

class Dog extends Animal {
	public void shout () {
		System.out.print("w w w");
	}
}

class Cat extends Animal {
	public void shout () {
		System.out.println("m m m");
	}
}
